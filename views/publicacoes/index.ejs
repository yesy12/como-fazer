<%- include('../header') %>
<br>
<button type="button" class="btn 
btn-light">
	<a href="/publicacoes/nova">Nova 
Publicação</a>
</button><br><br>

<table class="table table-hover 
table-striped">
  <thead>
    <tr>
      <th scope="col">Publicação</th>
      <th scope="col">Ações</th>
     </tr>
  </thead>

  <tbody>
<% publicacoes.forEach(publicacao => { %>
    <tr>
	<td>
		<%=  publicacao.titulo%> 
	</td>
 	<td>
		<a href="/publicacoes/editar/<%= categoria %>/<%= publicacao.id %>">Editar</a>
		<a href="/publicacoes/excluir/<%= categoria %>/<%= publicacao.id %>">Excluir</a>
	</td>
    </tr>
<% }) %>
  </tbody>
</table>






<%- include('../footer') %>
